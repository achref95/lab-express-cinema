<div>
    <h1>Cinema</h1>
    <h2>Click on the movie</h2>
    <section>
        {{#each item}}
        <div>
            <img src="{{image}}" alt="{{title}}" srcset="">
            <h3>{{title}}</h3>
            <h4><a href="/movie/{{id}}">See more</a></h4>
        </div>
        {{/each}}
    </section>
</div>